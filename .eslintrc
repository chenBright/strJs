{
  "ecmaFeatures": {
    "globalReturn": true,
    "modules": true,
    "regexYFlag": true,
    "regexUFlag": true
  },
  "extends": "airbnb-base/legacy",
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "globals": {
    "document": false,
    "escape": false,
    "navigator": false,
    "unescape": false,
    "window": false,
    "describe": true,
    "before": true,
    "it": true,
    "expect": true,
    "sinon": true
  },
  "parser": "babel-eslint",
  "plugins": [],
  "rules": {
    linebreak-style: 0,
    "one-var": 0,
    "semi": [2, "never"],
    "no-param-reassign": 0,
    "no-bitwise": 0,
    "no-underscore-dangle": 0,
    "func-names": 1,
    "radix": 1,
    "no-cond-assign": 1,
    "space-before-function-paren": 0,
    "no-restricted-syntax": 1,
    "no-mixed-operators": 0
  }
}
